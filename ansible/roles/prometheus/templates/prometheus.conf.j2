global:
  scrape_interval: 5s
  evaluation_interval: 10s

scrape_configs:
  - job_name: prometheus
    static_configs:
      - targets: ['localhost:9090']
  - job_name: backend
    static_configs:
      - targets: ['10.14.0.10:9100']
  - job_name: frontend
    static_configs:
      - targets: ['10.14.0.70:9100']


rule_files:
  - alert.rules.yml

alerting:
  alertmanagers:
  - static_configs:
    - targets:
       - 10.14.0.140:9093